{
  "[html,markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss,css]": {
    "editor.defaultFormatter": "stylelint.vscode-stylelint"
  },
  "[javascript,typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "stylelint.validate": ["css", "scss"]
}
